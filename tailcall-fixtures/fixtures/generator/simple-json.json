{
  "inputs": [
    {
      "curl": {
        "src": "https://jsonplaceholder.typicode.com/posts/1",
        "headers": {
          "Content-Type": "application/json",
          "Accept": "application/json"
        },
        "fieldName": "post"
      }
    },
    {
      "curl": {
        "src": "https://jsonplaceholder.typicode.com/posts",
        "fieldName": "posts"
      }
    },
    {
      "proto": {
        "src": "../protobuf/news.proto"
      }
    }
