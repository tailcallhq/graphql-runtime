---
source: src/core/generator/generator.rs
expression: config.to_sdl()
---
schema @server @upstream(baseURL: "http://jsonplaceholder.typicode.com") {
  query: Query
}

type Address {
  city: String
  geo: Geo
  street: String
  suite: String
  zipcode: String
}

type Company {
  bs: String
  catchPhrase: String
  name: String
}

type Geo {
  lat: String
  lng: String
}

type Query {
  root1: [Root2] @http(path: "/users")
  root2(p1: Int!): Root2 @http(path: "/users/{{.args.p1}}")
}

type Root2 {
  address: Address
  company: Company
  email: String
  id: Int
  name: String
  phone: String
  username: String
  website: String
}
