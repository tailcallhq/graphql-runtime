schema @server @upstream(baseURL: "https://jsonplaceholder.typicode.com") {
  query: Query
}

enum news__Status {
  DELETED
  DRAFT
  PUBLISHED
}

type Address {
  city: String
  geo: Geo
  street: String
  suite: String
  zipcode: String
}

type Company {
  bs: String
  catchPhrase: String
  name: String
}

