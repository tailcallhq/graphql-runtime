  DELETED
  DRAFT
  PUBLISHED
}

type News {
  body: String
  id: Int
  postImage: String
  status: Status
  title: String
}

type NewsData {
  news: [News]
}

type Query {
  news: NewsData! @grpc(url: "http://localhost:50051", method: "news.NewsService.GetAllNews")
}
